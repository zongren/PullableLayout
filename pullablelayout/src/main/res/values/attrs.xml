<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable
        name="pullablelayout_config">
        <attr name="pullablelayout_side">
            <flag name="TOP" value="1"/>
            <flag name="LEFT" value="2"/>
            <flag name="BOTTOM" value="4"/>
            <flag name="RIGHT" value="8"/>
        </attr>
        <attr name="pullablelayout_distanceRatio" format="float"/>
        <attr name="pullablelayout_verticalAngle" format="float"/>
        <attr name="pullablelayout_horizontalAngle" format="float"/>
    </declare-styleable>
</resources>