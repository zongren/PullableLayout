<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="me.zongren.example.pullablelayout">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:label="MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".PullableTextViewActivity"
            android:label="PullableTextViewActivity">
        </activity>
        <activity
            android:name=".PullableWebViewActivity"
            android:label="PullableWebViewActivity">
        </activity>
        <activity
            android:name=".PullableScrollViewActivity"
            android:label="PullableScrollViewActivity">
        </activity>
        <activity
            android:name=".PullableImageViewActivity"
            android:label="PullableImageViewActivity">
        </activity>
        <activity
            android:name=".PullableListViewActivity"
            android:label="PullableListViewActivity">
        </activity>
        <activity
            android:name=".PullableHorizontalScrollViewActivity"
            android:label="PullableHorizontalScrollViewActivity">
        </activity>
        <activity
            android:name=".PullableVerticalRecyclerViewActivity"
            android:label="PullableVerticalRecyclerViewActivity">
        </activity>
        <activity
            android:name=".PullableHorizontalRecyclerViewActivity"
            android:label="PullableHorizontalRecyclerViewActivity">
        </activity>
        <activity
            android:name=".PullableGridViewActivity"
            android:label="PullableGridViewActivity">
        </activity>
        <activity
            android:name=".PullableExpandableListViewActivity"
            android:label="PullableExpandableListViewActivity">
        </activity>
    </application>

</manifest>